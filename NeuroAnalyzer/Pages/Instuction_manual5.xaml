      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NeuroAnalyzer"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Instuction_manual5">

    <Grid Background="White">
        <Image Source="/Images/Background.png" Stretch="Fill"/>
        <Image Margin="0 0 700 380" Source="/Images/logo.png" Height="25" Width="80" Stretch="Fill"/>
        <Button Click="Button_Next_Click" Margin="0 330 0 0" Content="Начать тестирование" FontSize="20" Height="40" Width="250" Background="#FF2C99B0" BorderBrush="{x:Null}" Foreground="White"  Style="{DynamicResource RoundButton}"/>
        <TextBlock Margin="0,80,0,285" TextWrapping="Wrap" TextAlignment="Center" FontSize="25" ><Run Text="Сосредоточьтесь, расслабтесь, закройте глаза."/></TextBlock>
        <Frame x:Name="InstuctionFrame2" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
