<Page 
      x:Class="NeuroAnalyzer.Pages.GraphPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NeuroAnalyzer.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GraphPage"
      Loaded="GraphLoaded"
      SizeChanged="GridSizeChanged">

    <Grid Background="White">
        <Image Source="/Images/Background.png" Stretch="Fill" Grid.RowSpan="2"/>
        <Frame x:Name="MainFrame" Grid.RowSpan="2" NavigationUIVisibility="Hidden"></Frame>
        <Grid Name="SpectrumGrid" Margin="10,10,10,10"/>
        <Grid Name="GraphGrid" Margin="10,10,10,10"/>
    </Grid>
</Page>
